<div class="flex flex-col p-4">
      <h2 class="kanit">
        Most Played
      </h2>
      <hr class="mb-3">
      <div>
        <% @songs.each.with_index do |song, index| %>
          <% break if index >= 3 %>
          <a href="#">
            <img class="rounded-full w-12 my-1" src="<%= song.song_image_url%>" alt="<%= song.title %>" />
          </a>
        <% end %>
      </div>
    </div>